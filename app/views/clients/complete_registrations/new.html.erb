<%= form_for @client, as: :client, url: complete_registration_path, html: { class: "auth-form" } do |f| %>  <%= hidden_field_tag :user_client_id, @user_client.id %>
  <h2>Complete seu cadastro</h2>

  <%= f.label :cnpj %>
  <%= f.text_field :cnpj %>

  <%= f.label :razao_social %>
  <%= f.text_field :razao_social %>

  <%= f.label :company_name %>
  <%= f.text_field :company_name %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :cpf %>
  <%= f.text_field :cpf %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :number_address %>
  <%= f.text_field :number_address %>

  <%= f.submit 'Finalizar Cadastro' %>
<% end %>